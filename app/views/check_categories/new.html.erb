<% provide(:title, "新增检查类型") %> 
<%= render 'shared/home_left' %>
<%= form_for(@category,url:template_check_categories_path(@template)) do |f| %>
  <%= render 'shared/error_messages' ,:object => f.object%>

  <%= f.label :category,"检查类型名称" %>
  <%= f.text_field :category %>
  <%= f.label :des,"备注" %>
  <%= f.text_field :des %>
  <%= f.submit '新增检查类型' %> 
<% end %>

