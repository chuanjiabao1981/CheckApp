<% provide(:title, "首页") %>

<% if signed_in? %>
  <% if current_user.site_admin? %>
    <%= render 'main/site_admin_home' %>
  <% elsif current_user.zone_admin? %>
    <%= render 'shared/home_left' %>
  <% elsif current_user.zone_supervisor? %>
    <%= render 'main/zone_supervisor_home' %>
  <% elsif current_user.org_checker? %>
    <%= render 'main/org_checker_home' %>
  <% end %>
<% else %>
<% end %>
