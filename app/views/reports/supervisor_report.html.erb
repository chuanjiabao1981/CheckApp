<% provide(:title, "全部的督察报告|"+@organization.name) %> 
<ul class="breadcrumb">
  <%= render partial:'reports/nav_report_first_level',
      locals:{organization:@organization} %>	
  <li class="active"><%= "#{@organization.name}督察报告" %></li>
</ul>
<% if current_user.session.zone_admin? or current_user.session.site_admin? %>
	<% new_reports_num = @organization.get_all_new_supervisor_report.size %>
<% else %>
	<% new_reports_num = nil %>
<% end %>
<%= render partial:'reports/reports_index',
			locals: { legend_name:"#{@organization.name} 全部督察报告",
					   new_reports_num:new_reports_num,
					  reports:@supervisor_reports} %>

